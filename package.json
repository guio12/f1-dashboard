{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue --ignore-pattern dist/ && stylelint **/*.{vue,css} && prettier --check '**/*.?(ts|vue|css)'",
    "lint:fix": "eslint . --fix",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:e2e": "exec playwright test",
    "test:e2e:ui": "exec playwright test --ui"
  },
  "dependencies": {
    "@iconify-json/flag": "^1.1.52",
    "@nuxt/ui": "^2.16.0",
    "date-fns": "^3.6.0",
    "v-calendar": "^3.1.2",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/fonts": "^0.7.0",
    "@nuxt/test-utils": "^3.13.1",
    "@playwright/test": "^1.45.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^14.12.3",
    "nuxt": "^3.11.2",
    "nuxt3-leaflet": "^1.0.12",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "stylelint": "^16.5.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "vitest": "^1.6.0"
  }
}
